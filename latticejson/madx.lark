file: (_NEWLINE | line)*
line: element | lattice
element: NAME ":" type "," attributes
lattice: NAME ":" "LINE" "=" "(" names ")"
names: NAME ("," NAME)*
attributes: attribute ("," attribute)*
attribute: NAME "=" NUMBER

NAME: /[a-zA-Z_]\w*/
type: "CSBEND"
COMMENT: /![^\n]*/
_NEWLINE: ( /\r?\n[\t ]*/ | COMMENT )+
%import common.ESCAPED_STRING
%import common.SIGNED_NUMBER -> NUMBER
%import common.WS
%ignore WS
%ignore COMMENT
